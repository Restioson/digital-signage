{% extends "base.j2" %}
{% from "macros.j2" import content_stream %}

{% block title %}Upload Content{% endblock %}
{% block head %}
  {{ super() }}
  {{ css('config') }}
{% endblock %}

{% block content %}
  <script type="module">
    import {
      showContent,
    } from "{{ url_for('static', filename='config.mjs') }}";
    showContent({{content_stream_ids}});
  </script>
  <a href="/config/add_posts">Add a piece of content</a>
  <div class="column-list" id="root">
  </div>
{% endblock %}
